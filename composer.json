{
  "name": "llcbd/settings",
  "description": "Easy Persistent Settings Management for Laravel",
  "type": "package",
  "keywords": [
    "settings",
    "laravel",
    "lumen"
  ],
  "homepage": "http://www.letslearncoding.co",
  "require-dev": {
    "graham-campbell/testbench": "^3.3",
    "phpunit/phpunit": "^5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Lets Learn Coding",
      "email": "hello@letslearncoding.co"
    },
    {
      "name": "Muhammad Sumon Molla Selim",
      "email": "sumon@letslearncoding.co"
    }
  ],
  "autoload": {
    "psr-4": {
      "Llcbd\\Settings\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Llcbd\\Settings\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Llcbd\\Settings\\Providers\\SettingsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
